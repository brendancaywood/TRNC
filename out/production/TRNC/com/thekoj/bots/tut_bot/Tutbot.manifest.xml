<manifest>
    <main-class>com.thekoj.bots.tut_bot.Tutbot</main-class>
    <name>Tut Bot</name>
    <tag-line>Insert catchy tagline here (marketing oriented)</tag-line><!--Max of 50 chars-->
    <description>Does absolutely nothing</description><!--Max of 110 chars-->
    <version>1.0.2</version>
    <compatibility>
        <game>OSRS</game>
        <game>RS3</game>
    </compatibility>
    <categories>
        <category>DEVELOPER_TOOLS</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>Tutbot</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>Tree</tag>
        <tag>Vine</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>